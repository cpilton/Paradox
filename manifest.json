{
  "manifest_version": 2,
  "name": "Paradox",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-3.4.1.min.js",
        "content.js"
      ],
      "css": [
        "paradox.css"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "public/img/logo.png",
    "default_popup": "public/popup.html"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "paradox.js",
    "public/img/logo.png",
    "public/img/tick.svg",
    "public/img/warning.svg"
  ],

  "permissions": [
    "<all_urls>"
  ]
}